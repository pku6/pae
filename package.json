{
  "name": "pae",
  "version": "0.0.8",
  "main": "dist/main.js",
  "scripts": {
    "postinstall": "node dist/init.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^16.11.5",
    "electron": "^17.0.1"
  },
  "dependencies": {
    "@ddu6/cli-tools": "^0.0.22",
    "jsdom": "^18.0.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "^/info",
          "^/src",
          "^/tmp",
          "^/\\.gitignore",
          "^/config\\.json",
          "^/sessions\\.json"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ]
    }
  }
